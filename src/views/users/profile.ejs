<!-- head added -->
<%- include('../partials/head'); %>

<!-- header added -->
<%- include('../partials/header'); %>

        <main>
        <div class="container-profile">
            <div class="container-profile-logo">
            <img src=<%= user.imagen %>>
            </div>
            <form action="" class="container-form-profile">
                <div class="profile-container-email">
                    <div class="profile-input-description">Email:</div>
                    <input type="email" value=<%= user.email %> class="profile-email">
                </div>
                <div class="profile-container-name">
                    <div class="profile-input-description">Nombre:</div>
                    <input type="text" value=<%= user.nombre %> class="profile-name">
                </div>
                <div class="profile-container-lastname">
                    <div class="profile-input-description">Apellido:</div>
                    <input type="text" value=<%= user.apellido %> class="profile-lastname">
                </div>
                <div class="profile-container-tel">
                    <div class="profile-input-description">Telefono:</div>
                    <input type="text" value=<%= user.telefono %> class="profile-tel">
                </div>
                <div class="profile-container-prov">
                    <div class="profile-input-description">Provincia:</div>
                    <input type="text" value=<%= user.provincia %> class="profile-prov">
                </div>
                <div class="profile-container-city">
                    <div class="profile-input-description">Ciudad:</div>
                    <input type="text" value=<%= user.ciudad %> class="profile-city">
                </div>
                <div class="profile-container-street">
                    <div class="profile-input-description">Calle:</div>
                    <input type="text" value=<%= user.calle %> class="profile-street">
                </div>
                <div class="profile-container-number">
                    <div class="profile-input-description">Numero:</div>
                    <input type="text" value=<%= user.numero %> class="profile-number">
                </div>
                <div class="profile-container-cntry">
                    <div class="profile-input-description">Pais:</div>
                    <input type="text" value=<%= user.pais %> class="profile-cntry">
                </div>
                <div class="profile-container-zip">
                    <div class="profile-input-description">Codigo postal:</div>
                    <input type="text" value=<%= user.codigoPostal %> class="profile-zip">
                </div>
                <div class="profile-container-dni">
                    <div class="profile-input-description">DNI:</div>
                    <input type="text" value=<%= user.dni %> class="profile-dni">
                </div>
                <div class="profile-delete-button">
                    <input type="submit" class="button-perfil-delete" value="Eliminar usuario">
                </div>
                <div class="profile-change-button">
                    <input type="submit" class="button-perfil-change" value="Modificar usuario">
                </div>
            </form>
        </div>
</main>
<!-- footer added -->
<%- include('../partials/footer'); %>